{
  "name": "hris-restful-api",
  "version": "1.0.0",
  "description": "HRIS System Restful API",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "build:travis": "tsc -p tsconfig.build.json",
    "compodoc": "npx compodoc -p tsconfig.json",
    "docs": "compodoc -s -d ./documentation ",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "node --max_old_space_size=5500 ./node_modules/.bin/nodemon",
    "start:debug": "node --max_old_space_size=2500 ./node_modules/.bin/nodemon --config nodemon-debug.json",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -c development",
    "migration:docker": "ts-node node_modules/.bin/typeorm migration:run -c docker",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ormconfig.json",
    "prestart:prod": "rimraf dist && npx run build",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --detectOpenHandles --config ./test/jest-e2e.json",
    "test:e2e:all": "npm run test:e2e -- organisationunit.e2e-spec.ts && npm run test:e2e -- comprehensivetest.e2e-spec.ts && npm run test:e2e -- report.e2e-spec.ts && npm run test:e2e -- training.e2e-spec.ts && npm run test:e2e -- user.e2e-spec.ts && npm run test:e2e -- visualization.e2e-spec.ts && npm run test:e2e -- authentication.e2e-spec.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@iapps/period-utilities": "^1.0.0-beta.3",
    "@iapps/utils": "^1.0.1-alpha.5",
    "@icodebible/utils": "^1.5.9",
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.2.0",
    "@nestjs/microservices": "7.4.4",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "^7.2.0",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "7.1.4",
    "@nestjs/websockets": "7.2.0",
    "@tsed/common": "^5.57.7",
    "@tsed/core": "5.65.1",
    "@tsed/di": "^5.59.1",
    "@tsed/typeorm": "^6.0.0",
    "@types/pg": "^7.14.2",
    "apollo-server-express": "^2.15.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cron": "^1.8.2",
    "cron-parser": "^2.15.0",
    "csurf": "^1.11.0",
    "date-fns": "2.14.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "extract-zip": "^2.0.1",
    "handlebars": "^4.7.6",
    "helmet": "^4.1.1",
    "lodash": "^4.17.19",
    "madge": "^3.9.2",
    "method-override": "^3.0.0",
    "nestjs-session": "^1.0.1",
    "nestjs-typeorm-paginate": "^2.0.3",
    "node-pre-gyp": "^0.15.0",
    "node-stream-zip": "^1.11.2",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.25",
    "uid": "^1.0.0",
    "unzipper": "^0.10.11",
    "uuid": "^8.2.0",
    "yallist": "^4.0.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/testing": "7.2.0",
    "@types/cron": "^1.7.2",
    "@types/express": "4.17.6",
    "@types/jest": "^26.0.3",
    "@types/node": "14.0.14",
    "@types/supertest": "2.0.9",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "sqlite3": "^5.0.0",
    "supertest": "4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
  "migration:generate": "npx run typeorm migration:generate -n",
  "migration:create": "npx run typeorm migration:create -n",
  "migration:run": "npx run typeorm migration:run"
}
